<% include ../index1.ejs %>
<!-- Page Heading -->
<h1 class="h3 mb-2 text-gray-800"><%= title %></h1>
<div class="card shadow mb-4">
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                <tr>
                    <th>STT</th>
                    <th>Câu hỏi</th>
                    <th>Nội dung</th>
                    <th>Hành động</th>
                </tr>
                </thead>
                <tfoot>
                <tr>
                    <th>STT</th>
                    <th>Câu hỏi</th>
                    <th>Nội dung</th>
                    <th>Hành động</th>
                </tr>
                </tfoot>
                <tbody>
                <% questionn.forEach(function(item) { %>
                    <tr>
                        <td><%= item.id %></td>
                        <td><%= item.title_name %></td>
                        <td><%= item.name %></td>
                        <td></td>
                    </tr>
                <% }); %>
                </tbody>
            </table>
        </div>
    </div>
</div>
